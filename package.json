{
  "name": "personal-docs",
  "version": "1.0.0",
  "description": "[toc]",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "commit": "cz",
    "test:o": "jest -o",
    "test": "jest",
    "prepublish": "lerna run build --scope auto-add-subtitle",
    "publish": "lerna publish",
    "debug": "ts-node ./packages/auto-add-subtitle/src/veed-auto-add-title.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heiseshandian/personal-docs.git"
  },
  "keywords": [],
  "author": "gqzhang5 <gqzhang5@iflytek.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/heiseshandian/personal-docs/issues"
  },
  "homepage": "https://github.com/heiseshandian/personal-docs#readme",
  "devDependencies": {
    "typescript": "^4.0.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-lerna-scopes": "^11.0.0",
    "@types/jest": "^26.0.14",
    "@types/puppeteer": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "commitizen": "^4.2.1",
    "cz-lerna-changelog": "^2.0.3",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lerna": "^3.4.3",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "@types/progress": "^2.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e .git/COMMIT_EDITMSG",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  }
}
